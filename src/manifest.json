{
  "manifest_version": 2,
  "name": "Chronote",
  "permissions": [
    "activeTab"
  ],
  "description": "A note-taking extension for Chrome",
  "author": "Dave Kinsella",
  "version": "0.1",
  "browser_action": {
    "default_icon": {
      "128": "icon/noteicon128.png",
      "48": "icon/noteicon48.png",
      "19": "icon/noteicon19.png",
      "16": "icon/noteicon16.png"
    },
    "default_title": "Chronote - notes in Chrome",
    "default_popup": "popup.html"
  }
}